package <%= packageName %>.listener;

import <%= packageName %>.config.RabbitMQConfig<%= queueName %>;
import <%= packageName %>.domain.RabbitMessageModel;
import org.springframework.amqp.rabbit.annotation.RabbitListener;
import org.springframework.stereotype.Component;
import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

@Component
public class RabbitMQConsumer<%= queueName %> {

    private static final Logger logger = LoggerFactory.getLogger(RabbitMQConsumer<%= queueName %>.class);

    @RabbitListener(queues = RabbitMQConfig<%= queueName %>.QUEUE)
    public void listener(RabbitMessageModel message) {
        logger.info("Received message: {}", message);
    }
}
